<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" prefHeight="409.0" prefWidth="641.0" styleClass="bodybg2" stylesheets="@../CSS/Background.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.PrevencaoController">
   <children>
      <Pane layoutY="-1.0" prefHeight="39.0" prefWidth="641.0" style="-fx-background-color: #B22222;">
         <children>
            <Label layoutX="236.0" layoutY="10.0" text="FORMAS DE PREVENÇÃO" textFill="WHITE">
               <font>
                  <Font name="HP Simplified" size="17.0" />
               </font></Label>
         </children>
      </Pane>
      <Text fill="#b90000" layoutX="41.0" layoutY="95.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Como se prevenir do Coronavírus?">
         <font>
            <Font name="HP Simplified" size="18.0" />
         </font>
      </Text>
      <Text fill="WHITE" layoutX="41.0" layoutY="141.0" strokeType="OUTSIDE" strokeWidth="0.0" text="As recomendações de prevenção à COVID-19 são as seguintes:" textAlignment="JUSTIFY">
         <font>
            <Font name="HP Simplified" size="17.0" />
         </font>
      </Text>
      <Text fill="WHITE" layoutX="41.0" layoutY="180.0" strokeType="OUTSIDE" strokeWidth="0.0" text="• Lave com frequência as mãos até a altura dos punhos, com água e sabão, ou então higienize com álcool em gel 70%." textAlignment="JUSTIFY" wrappingWidth="558.766357421875">
         <font>
            <Font name="HP Simplified" size="17.0" />
         </font>
      </Text>
      <Text fill="WHITE" layoutX="41.0" layoutY="233.0" strokeType="OUTSIDE" strokeWidth="0.0" text="• Ao tossir ou espirrar, cubra nariz e boca com lenço ou com o braço, e não com as mãos." textAlignment="JUSTIFY" wrappingWidth="569.766357421875">
         <font>
            <Font name="HP Simplified" size="17.0" />
         </font>
      </Text>
      <Text fill="WHITE" layoutX="41.0" layoutY="283.0" strokeType="OUTSIDE" strokeWidth="0.0" text="• Evite tocar olhos, nariz e boca com as mãos não lavadas." textAlignment="JUSTIFY" wrappingWidth="569.766357421875">
         <font>
            <Font name="HP Simplified" size="17.0" />
         </font>
      </Text>
      <Text fill="WHITE" layoutX="41.0" layoutY="319.0" strokeType="OUTSIDE" strokeWidth="0.0" text="• Ao tocar, lave sempre as mãos como já indicado." textAlignment="JUSTIFY" wrappingWidth="569.766357421875">
         <font>
            <Font name="HP Simplified" size="17.0" />
         </font>
      </Text>
      <Button fx:id="btnProximo" layoutX="326.0" layoutY="356.0" mnemonicParsing="false" onAction="#btnProximo_Click" prefHeight="29.0" prefWidth="70.0" text="Próximo">
         <font>
            <Font name="HP Simplified" size="14.0" />
         </font>
      </Button>
      <Button fx:id="btnVoltar" layoutX="224.0" layoutY="356.0" mnemonicParsing="false" onAction="#btnVoltar_Click" prefHeight="29.0" prefWidth="70.0" text="Voltar">
         <font>
            <Font name="HP Simplified" size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
